def solution(arr):
    answer = []
    #arr=sorted(arr,reverse=True)
    cnt=min(arr)
    #print(cnt)
    
    arr.remove(cnt)
    
    if len(arr)==0:
        return [-1]
    else:
        #answer=arr
        return arr
        
        
        
